%%%
% This function makes a SPM Dpatch feature for the input image and type
% of detector. 
%%%

function [feat] = makeSPMFeature(img_name, img_path, save_path, detectors_fname,  detection_threshold, overwrite)

[decision] = makeVisEntDetFeature(img_name, img_path, save_path, detectors_fname,  detection_threshold, overwrite);

end